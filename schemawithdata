/****** Object:  StoredProcedure [dbo].[GetAnalysisOfTop10Jurisdiction]    Script Date: 1/4/2024 3:21:24 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[GetAnalysisOfTop10Jurisdiction]
@SelectType int=1,
@Periods NVARCHAR(MAX) = NULL,
@CurrencyType NVARCHAR(10)
AS
BEGIN
		SET NOCOUNT ON;
		DROP TABLE IF EXISTS #CbcFileUpload;

		DROP TABLE IF EXISTS #CbcReportDataCurrencyWise;
		SELECT ClientId,NameOfGroup,ReportableAccountingYear,Period,Currency,ConstituentEntity,CorporateTaxRate,TaxJurisdiction,
		CAST((UnrelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS UnrelatedPartyRevenue,
		CAST((RelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS RelatedPartyRevenue,
		CAST((TotalRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS TotalRevenue,
		CAST((ProfitLossBeforeIncomeTax * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS ProfitLossBeforeIncomeTax,
		CAST((IncomeTaxPaidOnCashBasis * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxPaidOnCashBasis,
		CAST((IncomeTaxAccruedRAY * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxAccruedRAY,
		CAST((StatedCapital * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS StatedCapital,
		CAST((AccumulatedEarnings * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS AccumulatedEarnings,
		NumberOfEmployees,TangibleAssets,FileName,FileFullName,CRD.CreatedBy,CRD.CreatedDate
		INTO #CbcReportDataCurrencyWise
		FROM CbcReportData AS CRD
		JOIN CbCRCurrencyConversionData AS CCD ON CRD.Currency=CCD.CurrencyFROM
		WHERE CCD.CurrencyTo=@CurrencyType AND ReportableAccountingYear = @Periods;

		SELECT
			TaxJurisdiction,
			SUM(TotalRevenue) AS JurisdictionRevenue,
			SUM(RelatedPartyRevenue) AS RelatedPartyRevenue,
			SUM(IncomeTaxAccruedRAY) AS IncomeTaxAccruedRAY,
			CASE
				WHEN SUM(ProfitLossBeforeIncomeTax) = 0 THEN 0.00
				ELSE CAST((CAST(SUM(IncomeTaxAccruedRAY) AS DECIMAL(19,2)) * 100 / CAST(SUM(ProfitLossBeforeIncomeTax) AS DECIMAL(19,2))) AS DECIMAL(18,2))
			END AS ETR,
			SUM(SUM(TotalRevenue)) OVER() AS GroupRevenue,
			SUM(SUM(RelatedPartyRevenue)) OVER() AS TotalRPR,
			SUM(SUM(IncomeTaxAccruedRAY)) OVER() AS TotalIncomeTaxAccrued,
			COUNT(COUNT(TaxJurisdiction)) OVER() AS TotalJurisdiction
		INTO #CbcFileUpload
		FROM #CbcReportDataCurrencyWise
		WHERE TotalRevenue <> 0
			AND TotalRevenue <> 0.00 
			AND ReportableAccountingYear = @Periods
		GROUP BY TaxJurisdiction;

		IF(@SelectType=1)
			BEGIN
				SELECT TOP( 10 ) TaxJurisdiction,
					JurisdictionRevenue,
					RelatedPartyRevenue
				FROM #CbcFileUpload
				ORDER BY  JurisdictionRevenue DESC;
			END
		ELSE
			BEGIN
				SELECT TOP( 10 ) TaxJurisdiction,
					CAST((CAST(JurisdictionRevenue AS DECIMAL(19,2)) * 100/ CAST(GroupRevenue AS DECIMAL(19,2)) )  AS DECIMAL(18,2)) AS RevenuePercentageGroupRevenue, 
					CAST((CAST(RelatedPartyRevenue AS DECIMAL(19,2)) * 100/ CAST(JurisdictionRevenue AS DECIMAL(19,2)) )  AS DECIMAL(18,2)) AS RPRPercentageJurisdictionRevenue, 
					CAST(ETR  AS DECIMAL(18,2)) AS ETR
				FROM #CbcFileUpload
				ORDER BY  JurisdictionRevenue DESC;
			END;

		SELECT DISTINCT 
		REPLACE(CONVERT(VARCHAR(MAX), CAST(GroupRevenue AS DECIMAL(18,2)), 1), '.00', '') AS TotalGroupRevenue,
		REPLACE(CONVERT(VARCHAR(MAX), CAST(TotalRPR AS DECIMAL(18,2)), 1), '.00', '') AS TotalRPR,
		REPLACE(CONVERT(VARCHAR(MAX), CAST(TotalIncomeTaxAccrued AS DECIMAL(18,2)), 1), '.00', '') AS TotalIncomeTaxAccrued,
		TotalJurisdiction 
		FROM #CbcFileUpload;

END
GO
/****** Object:  StoredProcedure [dbo].[GetAnalysisOfTop10JurisdictionDrillDown]    Script Date: 1/4/2024 3:21:24 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[GetAnalysisOfTop10JurisdictionDrillDown]
@SelectType int=1,
@DrillCountry NVARCHAR(100),
@Periods NVARCHAR(MAX) = NULL,
@CurrencyType NVARCHAR(10)
AS
BEGIN
	SET NOCOUNT ON;

		DROP TABLE IF EXISTS #CbcFileUpload;

		DROP TABLE IF EXISTS #CbcReportDataCurrencyWise;
		SELECT ClientId,NameOfGroup,ReportableAccountingYear,Period,Currency,ConstituentEntity,CorporateTaxRate,TaxJurisdiction,
		CAST((UnrelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS UnrelatedPartyRevenue,
		CAST((RelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS RelatedPartyRevenue,
		CAST((TotalRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS TotalRevenue,
		CAST((ProfitLossBeforeIncomeTax * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS ProfitLossBeforeIncomeTax,
		CAST((IncomeTaxPaidOnCashBasis * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxPaidOnCashBasis,
		CAST((IncomeTaxAccruedRAY * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxAccruedRAY,
		CAST((StatedCapital * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS StatedCapital,
		CAST((AccumulatedEarnings * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS AccumulatedEarnings,
		NumberOfEmployees,TangibleAssets,FileName,FileFullName,CRD.CreatedBy,CRD.CreatedDate
		INTO #CbcReportDataCurrencyWise
		FROM CbcReportData AS CRD
		JOIN CbCRCurrencyConversionData AS CCD ON CRD.Currency=CCD.CurrencyFROM
		WHERE CCD.CurrencyTo=@CurrencyType AND ReportableAccountingYear = @Periods;

		SELECT
			ConstituentEntity,
			TaxJurisdiction,
			SUM(TotalRevenue) AS JurisdictionRevenue,
			SUM(RelatedPartyRevenue) AS RelatedPartyRevenue,
			SUM(IncomeTaxAccruedRAY) AS IncomeTaxAccruedRAY,
			CASE
				WHEN SUM(ProfitLossBeforeIncomeTax) = 0 THEN 0.00
				ELSE CAST((CAST(SUM(IncomeTaxAccruedRAY) AS DECIMAL(19,2)) * 100 / CAST(SUM(ProfitLossBeforeIncomeTax) AS DECIMAL(19,2))) AS DECIMAL(18,2))
			END AS ETR,
			SUM(SUM(TotalRevenue)) OVER(PARTITION BY TaxJurisdiction) AS GroupRevenue,
			SUM(SUM(RelatedPartyRevenue)) OVER(PARTITION BY TaxJurisdiction) AS TotalRPR,
			SUM(SUM(IncomeTaxAccruedRAY)) OVER(PARTITION BY TaxJurisdiction) AS TotalIncomeTaxAccrued,
			COUNT(COUNT(ConstituentEntity)) OVER(PARTITION BY TaxJurisdiction) AS TotalConstituentEntity
		INTO #CbcFileUpload
		FROM #CbcReportDataCurrencyWise
		WHERE TotalRevenue <> 0
			AND TotalRevenue <> 0.00 
			AND ReportableAccountingYear = @Periods
			AND LOWER(TaxJurisdiction) = LOWER(@DrillCountry)
		GROUP BY TaxJurisdiction, ConstituentEntity;
		
		IF(@SelectType=1)
			BEGIN
				SELECT REPLACE(ConstituentEntity, ' ', '\n') AS ConstituentEntity, TaxJurisdiction,
					JurisdictionRevenue,
					RelatedPartyRevenue
				FROM #CbcFileUpload
				ORDER BY  JurisdictionRevenue DESC;
			END
		ELSE
			BEGIN
				SELECT REPLACE(ConstituentEntity, ' ', '\n') AS ConstituentEntity, TaxJurisdiction,
					CAST((CAST(JurisdictionRevenue AS DECIMAL(19,2)) * 100/ CAST(GroupRevenue AS DECIMAL(19,2)) )  AS DECIMAL(18,2)) AS RevenuePercentageGroupRevenue, 
					CAST((CAST(RelatedPartyRevenue AS DECIMAL(19,2)) * 100/ CAST(JurisdictionRevenue AS DECIMAL(19,2)) )  AS DECIMAL(18,2)) AS RPRPercentageJurisdictionRevenue, 
					CAST(ETR  AS DECIMAL(18,2)) AS ETR
				FROM #CbcFileUpload
				ORDER BY JurisdictionRevenue DESC;
			END;

		SELECT DISTINCT 
		REPLACE(CONVERT(VARCHAR(MAX), CAST(GroupRevenue AS DECIMAL(18,2)), 1), '.00', '') AS TotalGroupRevenue,
		REPLACE(CONVERT(VARCHAR(MAX), CAST(TotalRPR AS DECIMAL(18,2)), 1), '.00', '') AS TotalRPR,
		REPLACE(CONVERT(VARCHAR(MAX), CAST(TotalIncomeTaxAccrued AS DECIMAL(18,2)), 1), '.00', '') AS TotalIncomeTaxAccrued,
		TotalConstituentEntity 
		FROM #CbcFileUpload;

END
GO
/****** Object:  StoredProcedure [dbo].[GetCbCrFilterCountryMasterDataByPeriod]    Script Date: 1/4/2024 3:21:24 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[GetCbCrFilterCountryMasterDataByPeriod]
@Periods NVARCHAR(500)= NULL
AS
BEGIN
	SET NOCOUNT ON;

	DECLARE @PeriodsCount INT;
	DECLARE @ResultQuery NVARCHAR(MAX);

	DROP TABLE IF EXISTS #TempPeriods;
	CREATE TABLE #TempPeriods (Item NVARCHAR(255));

	INSERT INTO #TempPeriods (Item)
	SELECT Item
	FROM dbo.SplitStringToTable(@Periods, ',');

	SET @ResultQuery = N'SELECT TaxJurisdiction, SUM(TotalRevenue) AS TotalRevenue, ROW_NUMBER() OVER(ORDER BY SUM(TotalRevenue) DESC) AS TopNumber FROM CbcReportData WHERE 1 = 1 ';
	SELECT @PeriodsCount = COUNT(*) FROM #TempPeriods;

	IF @PeriodsCount > 0 
	BEGIN
			SET @ResultQuery +=' AND ReportableAccountingYear IN (SELECT Item FROM #TempPeriods)'
	END	

    SET @ResultQuery += ' GROUP BY TaxJurisdiction ORDER BY TotalRevenue DESC;';

	PRINT @ResultQuery;
	EXEC sp_executesql @ResultQuery;
END
GO
/****** Object:  StoredProcedure [dbo].[GetCbCrFilterCurrencyData]    Script Date: 1/4/2024 3:21:24 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[GetCbCrFilterCurrencyData]
AS
BEGIN
	SET NOCOUNT ON;

	SELECT DISTINCT Currency FROM CbcReportData;

	SELECT DISTINCT CurrencyCode, (CurrencyCode + ' ' + CurrencySymbol) AS CurrencyType 
	FROM  CountryMaster 
	WHERE CurrencyCode IN ('INR','USD','EUR','JPY','GBP','AUD','CAD','CHF','CNY','HKD') 
		AND ISNULL(CurrencyCode + ' ' + CurrencySymbol, '') <> '';
END
GO
