GO

CREATE PROCEDURE [dbo].[GetCbCrFilterEntityMasterDataByPeriodAndCountry]
@Periods NVARCHAR(500) = NULL,
@Countries NVARCHAR(MAX) = NULL
AS
BEGIN
	SET NOCOUNT ON;
	DECLARE @PeriodsCount INT;
	DECLARE @CountriesCount INT;

	DECLARE @ResultQuery NVARCHAR(MAX);

	DROP TABLE IF EXISTS #TempPeriods;
	DROP TABLE IF EXISTS #TempCountries;

	CREATE TABLE #TempPeriods (Item NVARCHAR(255));
	CREATE TABLE #TempCountries (Item NVARCHAR(255));

	INSERT INTO #TempPeriods (Item)
	SELECT Item
	FROM dbo.SplitStringToTable(@Periods, ',');

	INSERT INTO #TempCountries (Item)
	SELECT Item
	FROM dbo.SplitStringToTable(@Countries, ',');

	SET @ResultQuery = N'SELECT DISTINCT ConstituentEntity FROM CbcReportData WHERE 1 = 1 AND ConstituentEntity IS NOT NULL ';
	
	SELECT @PeriodsCount = COUNT(*) FROM #TempPeriods;
	SELECT @CountriesCount = COUNT(*) FROM #TempCountries;

	IF @PeriodsCount > 0 
	BEGIN
			SET @ResultQuery +=' AND ReportableAccountingYear IN (SELECT Item FROM #TempPeriods)'
	END	

	IF @CountriesCount > 0 
	BEGIN
			SET @ResultQuery +=' AND TaxJurisdiction IN (SELECT Item FROM #TempCountries)'
	END	

    SET @ResultQuery += ' ORDER BY ConstituentEntity;';

	PRINT @PeriodsCount;
	PRINT @CountriesCount;
	PRINT @ResultQuery;
	EXEC sp_executesql @ResultQuery;
END
GO
