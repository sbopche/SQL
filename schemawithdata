	[CorporateTaxRate] [decimal](7, 2) NULL,
	[TaxJurisdiction] [nvarchar](100) NOT NULL,
	[UnrelatedPartyRevenue] [bigint] NOT NULL,
	[RelatedPartyRevenue] [bigint] NOT NULL,
	[TotalRevenue] [bigint] NOT NULL,
