CREATE TABLE [dbo].[CbCRCurrencyConversionData](
	[Id] [bigint] IDENTITY(1,1) NOT NULL,
	[CurrencyFrom] [nvarchar](5) NULL,
	[CurrencyTo] [nvarchar](5) NULL,
	[CurrencyFromValue] [decimal](19, 9) NULL,
	[CurrencyToValue] [decimal](19, 9) NULL,
	[CreatedBy] [nvarchar](512) NULL,
	[ModifiedBy] [nvarchar](512) NULL,
	[CreationDate] [nvarchar](512) NULL,
	[ModifiedDate] [nvarchar](512) NULL,
