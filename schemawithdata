/****** Object:  StoredProcedure [dbo].[GetETRSnapshotOfnthYears]    Script Date: 1/4/2024 3:21:24 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[GetETRSnapshotOfnthYears]
@TopCountry INT = 50,
@Periods NVARCHAR(MAX) = NULL,
@Countries NVARCHAR(MAX) = NULL,
@Entities NVARCHAR(MAX) = NULL,
@CurrencyType NVARCHAR(10)
AS
BEGIN
		SET NOCOUNT ON;
		DECLARE @DYNAMICPIVOTQUERY AS NVARCHAR(MAX);
		DECLARE @PIVOTCOLUMNNAMES AS NVARCHAR(MAX);
		DECLARE @FYPERIODLIST AS NVARCHAR(MAX);
		DECLARE @CURRENTYEAR AS NVARCHAR(50) ='FY_' +  REPLACE(@Periods, '-', '_');

		DROP TABLE IF EXISTS #TempPeriods;
		DROP TABLE IF EXISTS #TempCountries;
		DROP TABLE IF EXISTS #TempEntities;

		DROP TABLE IF EXISTS #CbcReportDataCurrencyWise;
		SELECT ClientId,NameOfGroup,ReportableAccountingYear,Period,Currency,ConstituentEntity,CorporateTaxRate,TaxJurisdiction,
		CAST((UnrelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS UnrelatedPartyRevenue,
		CAST((RelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS RelatedPartyRevenue,
		CAST((TotalRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS TotalRevenue,
		CAST((ProfitLossBeforeIncomeTax * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS ProfitLossBeforeIncomeTax,
		CAST((IncomeTaxPaidOnCashBasis * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxPaidOnCashBasis,
		CAST((IncomeTaxAccruedRAY * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxAccruedRAY,
		CAST((StatedCapital * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS StatedCapital,
		CAST((AccumulatedEarnings * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS AccumulatedEarnings,
		NumberOfEmployees,TangibleAssets,FileName,FileFullName,CRD.CreatedBy,CRD.CreatedDate
		INTO #CbcReportDataCurrencyWise
		FROM CbcReportData AS CRD
		JOIN CbCRCurrencyConversionData AS CCD ON CRD.Currency=CCD.CurrencyFROM
		WHERE CCD.CurrencyTo=@CurrencyType;

		CREATE TABLE #TempPeriods (Item NVARCHAR(255));
		CREATE TABLE #TempCountries (Item NVARCHAR(255));
		CREATE TABLE #TempEntities (Item NVARCHAR(255));

		IF(@Periods IS NULL)
		BEGIN
			SELECT DISTINCT TOP 1 @Periods= ReportableAccountingYear FROM #CbcReportDataCurrencyWise  ORDER BY ReportableAccountingYear DESC;
		END
		
		IF(@Periods IS NOT NULL)
			BEGIN
				INSERT INTO #TempPeriods (Item)
				SELECT DISTINCT ReportableAccountingYear AS Item
				FROM #CbcReportDataCurrencyWise 
				WHERE CAST(SUBSTRING(ReportableAccountingYear, 1, 4) AS INT) BETWEEN CAST(SUBSTRING(@Periods, 1, 4) AS INT) - 5 AND CAST(SUBSTRING(@Periods, 1, 4) AS INT)
				ORDER BY ReportableAccountingYear DESC;
			END;
		
		IF(@Countries IS NOT NULL)
			BEGIN
				INSERT INTO #TempCountries (Item)
				SELECT Item
				FROM dbo.SplitStringToTable(@Countries, ',');
			END
		ELSE
			BEGIN
				INSERT INTO #TempCountries (Item)
				SELECT DISTINCT TaxJurisdiction AS Item
				FROM #CbcReportDataCurrencyWise 
				WHERE ReportableAccountingYear IN (SELECT Item FROM #TempPeriods);
			END;

		IF(@Entities IS NOT NULL)
			BEGIN
				INSERT INTO #TempEntities (Item)
				SELECT Item
				FROM dbo.SplitStringToTable(@Entities, ',');
			END
		ELSE
			BEGIN
				INSERT INTO #TempEntities (Item)
				SELECT DISTINCT ConstituentEntity AS Item
				FROM #CbcReportDataCurrencyWise
				WHERE TaxJurisdiction IN (SELECT Item FROM #TempCountries);
			END;

		-- Get Distinct FY Period For dynamic Pivot...
		SELECT @FYPERIODLIST= COALESCE(@FYPERIODLIST+',','')+ QUOTENAME(Item) FROM #TempPeriods;

		-- Set Pivot Column Names
		SET @PIVOTCOLUMNNAMES = STUFF((
					SELECT DISTINCT ',' + 'ISNULL(' + QUOTENAME(Item)+ ', 0) AS FY_' +  REPLACE(Item, '-', '_')
					FROM #TempPeriods 
					FOR XML PATH('')
				), 1, 1, '');
		
		-- Set Dynamic Pivot Query
		SET @DYNAMICPIVOTQUERY = N'
		WITH ReportableAccountingYearCTE AS (
			SELECT ReportableAccountingYear, TaxJurisdiction, 
			ROUND((CASE
					WHEN SUM(ProfitLossBeforeIncomeTax) = 0 
					THEN 0.00
					ELSE CAST((CAST(SUM(IncomeTaxAccruedRAY) AS DECIMAL(19,2)) * 100 / CAST(SUM(ProfitLossBeforeIncomeTax) AS DECIMAL(19,2))) AS DECIMAL(18,2))
			END), 2) AS ETR
			FROM #CbcReportDataCurrencyWise 
			WHERE TaxJurisdiction IN (SELECT Item FROM #TempCountries) AND (ConstituentEntity IN (SELECT Item FROM #TempEntities) OR ConstituentEntity IS NULL)
			GROUP BY ReportableAccountingYear, TaxJurisdiction
		)
		SELECT * FROM (
			SELECT TOP(' + CONVERT(NVARCHAR(255), @TopCountry )+ ') TaxJurisdiction, ' + @PIVOTCOLUMNNAMES + '
			FROM ReportableAccountingYearCTE
			PIVOT (MAX(ETR) FOR ReportableAccountingYear IN (' + @FYPERIODLIST + ')) AS PivotTable
		) AS PV ORDER BY ' + @CURRENTYEAR+ ' DESC ;';

		PRINT @DYNAMICPIVOTQUERY;
		EXEC sp_executesql @DYNAMICPIVOTQUERY;

		
		WITH CbcReportDataCountCTE AS (
			SELECT DISTINCT
			ReportableAccountingYear,
			TaxJurisdiction,
			SUM(TotalRevenue) AS TotalRevenuePerTaxJurisdiction,
			SUM(IncomeTaxAccruedRAY) AS IncomeTaxAccruedRAYPerTaxJurisdiction,
			SUM(SUM(IncomeTaxAccruedRAY)) OVER(PARTITION BY ReportableAccountingYear) AS IncomeTaxAccruedRAYPerYear,
			SUM(SUM(TotalRevenue)) OVER (PARTITION BY ReportableAccountingYear) AS TotalRevenuePerYear
			FROM #CbcReportDataCurrencyWise
			WHERE TotalRevenue <> 0 
					AND ReportableAccountingYear = @Periods
					AND TaxJurisdiction IN (SELECT Item FROM #TempCountries) 
					AND (ConstituentEntity IN (SELECT Item FROM #TempEntities) OR ConstituentEntity IS NULL)
			GROUP BY ReportableAccountingYear, TaxJurisdiction
		)
		SELECT ReportableAccountingYear, 
		SUM(TotalRevenuePerTaxJurisdiction) AS TotalRevenueOfAllJurisdictionPerYear,
		SUM(IncomeTaxAccruedRAYPerTaxJurisdiction) AS IncomeTaxAccruedRAYPerYear,
		CAST((SUM(TotalRevenuePerTaxJurisdiction) * 100/ TotalRevenuePerYear) AS DECIMAL(20,2)) AS YearlyRevenuePercentageOfGroupRevenue,
		COUNT(TaxJurisdiction) AS TotalTaxJurisdictions
		FROM CbcReportDataCountCTE
		GROUP BY ReportableAccountingYear, TotalRevenuePerYear, IncomeTaxAccruedRAYPerYear
		ORDER BY ReportableAccountingYear DESC;

END
GO
/****** Object:  StoredProcedure [dbo].[GetETRSnapshotOfnthYearsDrillDown]    Script Date: 1/4/2024 3:21:24 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[GetETRSnapshotOfnthYearsDrillDown]
@DrillCountry NVARCHAR(100),
@Periods NVARCHAR(MAX) = NULL,
@Countries NVARCHAR(MAX) = NULL,
@Entities NVARCHAR(MAX) = NULL,
@CurrencyType NVARCHAR(10)
AS
BEGIN

		SET NOCOUNT ON;
		DECLARE @DYNAMICPIVOTQUERY AS NVARCHAR(MAX);
		DECLARE @PIVOTCOLUMNNAMES AS NVARCHAR(MAX);
		DECLARE @FYPERIODLIST AS NVARCHAR(MAX);
		DECLARE @CURRENTYEAR AS NVARCHAR(50) ='FY_' +  REPLACE(@Periods, '-', '_');

		DROP TABLE IF EXISTS #TempPeriods;
		DROP TABLE IF EXISTS #TempCountries;
		DROP TABLE IF EXISTS #TempEntities;

		DROP TABLE IF EXISTS #CbcReportDataCurrencyWise;
		SELECT ClientId,NameOfGroup,ReportableAccountingYear,Period,Currency,ConstituentEntity,CorporateTaxRate,TaxJurisdiction,
		CAST((UnrelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS UnrelatedPartyRevenue,
		CAST((RelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS RelatedPartyRevenue,
		CAST((TotalRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS TotalRevenue,
		CAST((ProfitLossBeforeIncomeTax * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS ProfitLossBeforeIncomeTax,
		CAST((IncomeTaxPaidOnCashBasis * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxPaidOnCashBasis,
		CAST((IncomeTaxAccruedRAY * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxAccruedRAY,
		CAST((StatedCapital * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS StatedCapital,
		CAST((AccumulatedEarnings * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS AccumulatedEarnings,
		NumberOfEmployees,TangibleAssets,FileName,FileFullName,CRD.CreatedBy,CRD.CreatedDate
		INTO #CbcReportDataCurrencyWise
		FROM CbcReportData AS CRD
		JOIN CbCRCurrencyConversionData AS CCD ON CRD.Currency=CCD.CurrencyFROM
		WHERE CCD.CurrencyTo=@CurrencyType;

		CREATE TABLE #TempPeriods (Item NVARCHAR(255));
		CREATE TABLE #TempCountries (Item NVARCHAR(255));
		CREATE TABLE #TempEntities (Item NVARCHAR(255));

		IF(@Periods IS NULL)
		BEGIN
			SELECT DISTINCT TOP 1 @Periods= ReportableAccountingYear FROM #CbcReportDataCurrencyWise  ORDER BY ReportableAccountingYear DESC;
		END
		
		IF(@Periods IS NOT NULL)
			BEGIN
				INSERT INTO #TempPeriods (Item)
				SELECT DISTINCT ReportableAccountingYear AS Item
				FROM #CbcReportDataCurrencyWise 
				WHERE CAST(SUBSTRING(ReportableAccountingYear, 1, 4) AS INT) BETWEEN CAST(SUBSTRING(@Periods, 1, 4) AS INT) - 5 AND CAST(SUBSTRING(@Periods, 1, 4) AS INT)
				ORDER BY ReportableAccountingYear DESC;
			END;
		
		IF(@Countries IS NOT NULL)
			BEGIN
				INSERT INTO #TempCountries (Item)
				SELECT Item
				FROM dbo.SplitStringToTable(@Countries, ',');
			END
		ELSE
			BEGIN
				INSERT INTO #TempCountries (Item)
				SELECT DISTINCT TaxJurisdiction AS Item
				FROM #CbcReportDataCurrencyWise
				WHERE ReportableAccountingYear IN (SELECT Item FROM #TempPeriods);
			END;

		IF(@Entities IS NOT NULL)
			BEGIN
				INSERT INTO #TempEntities (Item)
				SELECT Item
				FROM dbo.SplitStringToTable(@Entities, ',');
			END
		ELSE
			BEGIN
				INSERT INTO #TempEntities (Item)
				SELECT DISTINCT ConstituentEntity AS Item
				FROM #CbcReportDataCurrencyWise
				WHERE TaxJurisdiction IN (SELECT Item FROM #TempCountries);
			END;

		-- Get Distinct FY Period For dynamic Pivot...
		SELECT @FYPERIODLIST= COALESCE(@FYPERIODLIST+',','')+ QUOTENAME(Item) FROM #TempPeriods;

		-- Set Pivot Column Names
		SET @PIVOTCOLUMNNAMES = STUFF((
					SELECT DISTINCT ',' + 
					'ISNULL(' + QUOTENAME(Item)+ ', 0) AS FY_' +  REPLACE(Item, '-', '_')
					FROM #TempPeriods 
					FOR XML PATH('')
				), 1, 1, '');
		
		-- Set Dynamic Pivot Query
		SET @DYNAMICPIVOTQUERY = N'
		SELECT REPLACE(ConstituentEntity, '' '', '+'''\n'''+') AS ConstituentEntity, TaxJurisdiction, ' + @PIVOTCOLUMNNAMES + '
		FROM (
			SELECT ConstituentEntity, ReportableAccountingYear, TaxJurisdiction, 
			ROUND((CASE
					WHEN ProfitLossBeforeIncomeTax = 0 
					THEN 0.00
					ELSE CAST((CAST(IncomeTaxAccruedRAY AS DECIMAL(19,2)) * 100 / CAST(ProfitLossBeforeIncomeTax AS DECIMAL(19,2))) AS DECIMAL(18,2))
			END), 2) AS ETR
			FROM #CbcReportDataCurrencyWise 
			WHERE TaxJurisdiction IN (SELECT Item FROM #TempCountries) AND (ConstituentEntity IN (SELECT Item FROM #TempEntities) OR ConstituentEntity IS NULL)
		) AS SubQuery
		PIVOT (MAX(ETR) FOR ReportableAccountingYear IN (' + @FYPERIODLIST + ')) AS PivotTable
		WHERE LOWER(TaxJurisdiction) = LOWER('''+ @DrillCountry +''')
		ORDER BY ' + @CURRENTYEAR+ ' DESC ;';

		PRINT @DYNAMICPIVOTQUERY;
		EXEC sp_executesql @DYNAMICPIVOTQUERY;

		WITH CbcReportDataCountCTE AS (
			SELECT DISTINCT
			ReportableAccountingYear,
			TaxJurisdiction,
			SUM(TotalRevenue) AS TotalRevenuePerTaxJurisdiction,
			SUM(IncomeTaxAccruedRAY) AS IncomeTaxAccruedRAYPerTaxJurisdiction,
			SUM(SUM(IncomeTaxAccruedRAY)) OVER(PARTITION BY ReportableAccountingYear) AS IncomeTaxAccruedRAYPerYear,
			SUM(SUM(TotalRevenue)) OVER (PARTITION BY ReportableAccountingYear) AS TotalRevenuePerYear
			FROM #CbcReportDataCurrencyWise
			WHERE TotalRevenue <> 0 
					AND ReportableAccountingYear = @Periods
					AND TaxJurisdiction IN (SELECT Item FROM #TempCountries) 
					AND (ConstituentEntity IN (SELECT Item FROM #TempEntities) OR ConstituentEntity IS NULL)
			GROUP BY ReportableAccountingYear, TaxJurisdiction
		)
		SELECT ReportableAccountingYear, 
		SUM(TotalRevenuePerTaxJurisdiction) AS TotalRevenueOfAllJurisdictionPerYear,
		SUM(IncomeTaxAccruedRAYPerTaxJurisdiction) AS IncomeTaxAccruedRAYPerYear,
		CAST((SUM(TotalRevenuePerTaxJurisdiction) * 100/ TotalRevenuePerYear) AS DECIMAL(20,2)) AS YearlyRevenuePercentageOfGroupRevenue,
		COUNT(TaxJurisdiction) AS TotalTaxJurisdictions
		FROM CbcReportDataCountCTE
		WHERE LOWER(TaxJurisdiction) = LOWER(@DrillCountry)
		GROUP BY ReportableAccountingYear, TotalRevenuePerYear, IncomeTaxAccruedRAYPerYear
		ORDER BY ReportableAccountingYear DESC;

END
GO
/****** Object:  StoredProcedure [dbo].[GetOverviewOfTop10Jurisdiction]    Script Date: 1/4/2024 3:21:24 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[GetOverviewOfTop10Jurisdiction]
@TopCountry int=10,
@Periods NVARCHAR(MAX) = NULL,
@CurrencyType NVARCHAR(10)
AS
BEGIN
		SET NOCOUNT ON;
		DROP TABLE IF EXISTS #TempPeriods;
		DROP TABLE IF EXISTS #CbcFileUpload;

		DROP TABLE IF EXISTS #CbcReportDataCurrencyWise;
		SELECT ClientId,NameOfGroup,ReportableAccountingYear,Period,Currency,ConstituentEntity,CorporateTaxRate,TaxJurisdiction,
		CAST((UnrelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS UnrelatedPartyRevenue,
		CAST((RelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS RelatedPartyRevenue,
		CAST((TotalRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS TotalRevenue,
		CAST((ProfitLossBeforeIncomeTax * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS ProfitLossBeforeIncomeTax,
		CAST((IncomeTaxPaidOnCashBasis * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxPaidOnCashBasis,
		CAST((IncomeTaxAccruedRAY * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxAccruedRAY,
		CAST((StatedCapital * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS StatedCapital,
		CAST((AccumulatedEarnings * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS AccumulatedEarnings,
		NumberOfEmployees,TangibleAssets,FileName,FileFullName,CRD.CreatedBy,CRD.CreatedDate
		INTO #CbcReportDataCurrencyWise
		FROM CbcReportData AS CRD
		JOIN CbCRCurrencyConversionData AS CCD ON CRD.Currency=CCD.CurrencyFROM
		WHERE CCD.CurrencyTo=@CurrencyType;

		CREATE TABLE #TempPeriods (Item NVARCHAR(255));
		
		IF(@Periods IS NOT NULL)
			BEGIN
				INSERT INTO #TempPeriods (Item)
				SELECT Item
				FROM dbo.SplitStringToTable(@Periods, ',');
			END
		ELSE
			BEGIN
				INSERT INTO #TempPeriods (Item)
				SELECT DISTINCT ReportableAccountingYear AS Item
				FROM #CbcReportDataCurrencyWise;
			END;
		
		SELECT
			TaxJurisdiction,
			SUM(TotalRevenue) AS JurisdictionRevenue,
			SUM(RelatedPartyRevenue) AS RelatedPartyRevenue,
			SUM(IncomeTaxAccruedRAY) AS IncomeTaxAccruedRAY,
			ROUND((CASE
				WHEN SUM(ProfitLossBeforeIncomeTax) = 0 THEN 0
				ELSE (SUM(IncomeTaxAccruedRAY) / SUM(ProfitLossBeforeIncomeTax)) * 100
			END), 2) AS ETR,
			ROUND((CASE
					WHEN SUM(ProfitLossBeforeIncomeTax) = 0  THEN 0
					ELSE (SUM(ProfitLossBeforeIncomeTax)/ SUM( TotalRevenue ) * 100) 
			END), 2) AS PBT,
			SUM(SUM(TotalRevenue)) OVER() AS GroupRevenue,
			SUM(SUM(RelatedPartyRevenue)) OVER() AS TotalRPR,
			SUM(SUM(IncomeTaxAccruedRAY)) OVER() AS TotalIncomeTaxAccrued,
			COUNT(COUNT(TaxJurisdiction)) OVER() AS TotalJurisdiction
		INTO #CbcFileUpload
		FROM #CbcReportDataCurrencyWise
		WHERE TotalRevenue <> 0
			AND TotalRevenue <> 0.00 
			AND ReportableAccountingYear IN (SELECT Item FROM #TempPeriods)
		GROUP BY TaxJurisdiction
		ORDER BY JurisdictionRevenue DESC;
		
		SELECT TOP( @TopCountry ) 
			TaxJurisdiction,
			JurisdictionRevenue,
			CAST(ROUND(ETR, 2) AS DECIMAL(18,2)) AS ETR,
			CAST(ROUND(PBT, 2) AS DECIMAL(18,2)) AS PBT,
			REPLACE(CONVERT(VARCHAR, CAST(JurisdictionRevenue AS MONEY), 1), '.00', '') AS JurisdictionRevenueMoney,
			CAST(ROUND((JurisdictionRevenue / (GroupRevenue) *100), 2) AS DECIMAL(18,2)) AS RevenuePercentageGroupRevenue, 
			CAST(ROUND((RelatedPartyRevenue / ( JurisdictionRevenue ) *100), 2) AS DECIMAL(18,2)) AS RPRPercentageJurisdictionRevenue
		FROM #CbcFileUpload
		ORDER BY JurisdictionRevenue DESC;

		SELECT DISTINCT 
		REPLACE(CONVERT(VARCHAR(MAX), CAST(GroupRevenue AS DECIMAL(18,2)), 1), '.00', '') AS TotalGroupRevenue,
		REPLACE(CONVERT(VARCHAR(MAX), CAST(TotalRPR AS DECIMAL(18,2)), 1), '.00', '') AS TotalRPR,
		REPLACE(CONVERT(VARCHAR(MAX), CAST(TotalIncomeTaxAccrued AS DECIMAL(18,2)), 1), '.00', '') AS TotalIncomeTaxAccrued,
		TotalJurisdiction 
		FROM #CbcFileUpload;

END
GO
/****** Object:  StoredProcedure [dbo].[GetRelatedPartyRevenueSnapshotOfnthYears]    Script Date: 1/4/2024 3:21:24 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[GetRelatedPartyRevenueSnapshotOfnthYears]
@SelectType INT,
@Periods NVARCHAR(MAX) = NULL,
@Countries NVARCHAR(MAX) = NULL,
@Entities NVARCHAR(MAX) = NULL,
@CurrencyType NVARCHAR(10)
AS
BEGIN
		SET NOCOUNT ON;
		DECLARE @DYNAMICPIVOTQUERYREVENUE AS NVARCHAR(MAX);
		DECLARE @DYNAMICPIVOTQUERYPERCENTAGE AS NVARCHAR(MAX);
		DECLARE @PIVOTCOLUMNNAMESREVENUE AS NVARCHAR(MAX);
		DECLARE @PIVOTCOLUMNNAMESPERCENTAGE AS NVARCHAR(MAX);
		DECLARE @FYPERIODLIST AS NVARCHAR(MAX);
		DECLARE @CURRENTYEAR AS NVARCHAR(50) ='FY_' +  REPLACE(@Periods, '-', '_');

		DROP TABLE IF EXISTS #TempPeriods;
		DROP TABLE IF EXISTS #TempCountries;
		DROP TABLE IF EXISTS #TempEntities;
		DROP TABLE IF EXISTS #CbcFileUpload;
		DROP TABLE IF EXISTS #CbcFileUpload1;

		DROP TABLE IF EXISTS #CbcReportDataCurrencyWise;
		SELECT ClientId,NameOfGroup,ReportableAccountingYear,Period,Currency,ConstituentEntity,CorporateTaxRate,TaxJurisdiction,
		CAST((UnrelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS UnrelatedPartyRevenue,
		CAST((RelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS RelatedPartyRevenue,
		CAST((TotalRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS TotalRevenue,
		CAST((ProfitLossBeforeIncomeTax * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS ProfitLossBeforeIncomeTax,
		CAST((IncomeTaxPaidOnCashBasis * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxPaidOnCashBasis,
		CAST((IncomeTaxAccruedRAY * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxAccruedRAY,
		CAST((StatedCapital * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS StatedCapital,
		CAST((AccumulatedEarnings * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS AccumulatedEarnings,
		NumberOfEmployees,TangibleAssets,FileName,FileFullName,CRD.CreatedBy,CRD.CreatedDate
		INTO #CbcReportDataCurrencyWise
		FROM CbcReportData AS CRD
		JOIN CbCRCurrencyConversionData AS CCD ON CRD.Currency=CCD.CurrencyFROM
		WHERE CCD.CurrencyTo=@CurrencyType;

		CREATE TABLE #TempPeriods (Item NVARCHAR(255));
		CREATE TABLE #TempCountries (Item NVARCHAR(255));
		CREATE TABLE #TempEntities (Item NVARCHAR(255));

		IF(@Periods IS NULL)
		BEGIN
			SELECT DISTINCT TOP 1 @Periods= ReportableAccountingYear FROM #CbcReportDataCurrencyWise  ORDER BY ReportableAccountingYear DESC;
		END
		
		IF(@Periods IS NOT NULL)
			BEGIN
				INSERT INTO #TempPeriods (Item)
				SELECT DISTINCT ReportableAccountingYear AS Item
				FROM #CbcReportDataCurrencyWise 
				WHERE CAST(SUBSTRING(ReportableAccountingYear, 1, 4) AS INT) BETWEEN CAST(SUBSTRING(@Periods, 1, 4) AS INT) - 5 AND CAST(SUBSTRING(@Periods, 1, 4) AS INT)
				ORDER BY ReportableAccountingYear DESC;
			END;
		
		IF(@Countries IS NOT NULL)
			BEGIN
				INSERT INTO #TempCountries (Item)
				SELECT Item
				FROM dbo.SplitStringToTable(@Countries, ',');
			END
		ELSE
			BEGIN
				INSERT INTO #TempCountries (Item)
				SELECT DISTINCT TaxJurisdiction AS Item
				FROM #CbcReportDataCurrencyWise 
				WHERE ReportableAccountingYear IN (SELECT Item FROM #TempPeriods);
			END;

		IF(@Entities IS NOT NULL)
			BEGIN
				INSERT INTO #TempEntities (Item)
				SELECT Item
				FROM dbo.SplitStringToTable(@Entities, ',');
			END
		ELSE
			BEGIN
				INSERT INTO #TempEntities (Item)
				SELECT DISTINCT ConstituentEntity AS Item
				FROM #CbcReportDataCurrencyWise
				WHERE TaxJurisdiction IN (SELECT Item FROM #TempCountries);
			END;

		--BY VALUE TYPE
		IF(@SelectType = 1)
		BEGIN
			-- Created  Temp Table...
			SELECT DISTINCT ReportableAccountingYear, TaxJurisdiction, 
			SUM(RelatedPartyRevenue) AS RPROfJurisdiction
			INTO #CbcFileUpload
			FROM #CbcReportDataCurrencyWise 
			WHERE TotalRevenue <> 0 
				AND TaxJurisdiction IN (SELECT Item FROM #TempCountries) 
				AND (ConstituentEntity IN (SELECT Item FROM #TempEntities) OR ConstituentEntity IS NULL)
			GROUP BY ReportableAccountingYear, TaxJurisdiction 
			ORDER BY ReportableAccountingYear, TaxJurisdiction DESC;

			-- Get Distinct FY Period For dynamic Pivot...
			SELECT @FYPERIODLIST= COALESCE(@FYPERIODLIST+',','')+ QUOTENAME(ReportableAccountingYear) 
			FROM (SELECT DISTINCT ReportableAccountingYear FROM #CbcFileUpload) AS SUBQUERY;

			-- Set Pivot Column Names Revenue
			SET @PIVOTCOLUMNNAMESREVENUE = STUFF((
						SELECT DISTINCT ',' + 
						'ISNULL(' + QUOTENAME(ReportableAccountingYear)+ ', 0) AS FY_' +  REPLACE(ReportableAccountingYear, '-', '_')
						FROM (SELECT DISTINCT ReportableAccountingYear FROM #CbcFileUpload) AS SUBQUERY
						FOR XML PATH('')
					), 1, 1, '');

			-- Set Dynamic Pivot Query Revenue
			SET @DYNAMICPIVOTQUERYREVENUE = N'
				SELECT * FROM (
					SELECT DISTINCT TaxJurisdiction, ' + @PIVOTCOLUMNNAMESREVENUE + '
					FROM #CbcFileUpload
					PIVOT (MAX(RPROfJurisdiction) FOR ReportableAccountingYear IN (' + @FYPERIODLIST + ')) AS PivotTable
				) AS PV ORDER BY ' + @CURRENTYEAR+ ' DESC ;';

			PRINT @DYNAMICPIVOTQUERYREVENUE;
			EXEC sp_executesql @DYNAMICPIVOTQUERYREVENUE;
		END
		ELSE
		BEGIN
			-- Created  Temp1 Table...
			SELECT DISTINCT ReportableAccountingYear, TaxJurisdiction, 
			CAST(((CAST(SUM(RelatedPartyRevenue) AS DECIMAL(19,2)) * 100 / CAST(SUM(TotalRevenue) AS DECIMAL(19,2)))) AS DECIMAL(19,2)) AS RPRPercentageOfJurisdictionRevenue 
			INTO #CbcFileUpload1
			FROM #CbcReportDataCurrencyWise 
			WHERE TotalRevenue <> 0 
				AND TaxJurisdiction IN (SELECT Item FROM #TempCountries) 
				AND (ConstituentEntity IN (SELECT Item FROM #TempEntities) OR ConstituentEntity IS NULL)
			GROUP BY ReportableAccountingYear, TaxJurisdiction 
			ORDER BY ReportableAccountingYear, TaxJurisdiction DESC;

			-- Get Distinct FY Period For dynamic Pivot...
			SELECT @FYPERIODLIST= COALESCE(@FYPERIODLIST+',','')+ QUOTENAME(ReportableAccountingYear) 
			FROM (SELECT DISTINCT ReportableAccountingYear FROM #CbcFileUpload1) AS SUBQUERY;

			-- Set Pivot Column Names Percentage
			SET @PIVOTCOLUMNNAMESPERCENTAGE = STUFF((
						SELECT DISTINCT ',' + 
						'ISNULL(' + QUOTENAME(ReportableAccountingYear)+ ', 0) AS FY_' +  REPLACE(ReportableAccountingYear, '-', '_')
						FROM (SELECT DISTINCT ReportableAccountingYear FROM #CbcFileUpload1) AS SUBQUERY
						FOR XML PATH('')
					), 1, 1, '');

			-- Set Dynamic Pivot Query Precentage
			SET @DYNAMICPIVOTQUERYPERCENTAGE = N'
				SELECT * FROM (
					SELECT DISTINCT TaxJurisdiction, ' + @PIVOTCOLUMNNAMESPERCENTAGE + '
					FROM #CbcFileUpload1
					PIVOT (MAX(RPRPercentageOfJurisdictionRevenue) FOR ReportableAccountingYear IN (' + @FYPERIODLIST + ')) AS PivotTable
				) AS PV ORDER BY ' + @CURRENTYEAR+ ' DESC ;';

			PRINT @DYNAMICPIVOTQUERYPERCENTAGE;
			EXEC sp_executesql @DYNAMICPIVOTQUERYPERCENTAGE;
		END;
		
		
		WITH CbcReportDataCountCTE AS (
			SELECT DISTINCT
			ReportableAccountingYear,
			TaxJurisdiction,
			SUM(TotalRevenue) AS TotalRevenuePerTaxJurisdiction,
			SUM(IncomeTaxAccruedRAY) AS IncomeTaxAccruedRAYPerTaxJurisdiction,
			SUM(SUM(IncomeTaxAccruedRAY)) OVER(PARTITION BY ReportableAccountingYear) AS IncomeTaxAccruedRAYPerYear,
			SUM(SUM(TotalRevenue)) OVER (PARTITION BY ReportableAccountingYear) AS TotalRevenuePerYear
			FROM #CbcReportDataCurrencyWise
			WHERE TotalRevenue <> 0 
					AND ReportableAccountingYear = @Periods
					AND TaxJurisdiction IN (SELECT Item FROM #TempCountries) 
					AND (ConstituentEntity IN (SELECT Item FROM #TempEntities) OR ConstituentEntity IS NULL)
			GROUP BY ReportableAccountingYear, TaxJurisdiction
		)
		SELECT ReportableAccountingYear,
		SUM(TotalRevenuePerTaxJurisdiction) AS TotalRevenueOfAllJurisdictionPerYear,
		SUM(IncomeTaxAccruedRAYPerTaxJurisdiction) AS IncomeTaxAccruedRAYPerYear,
		CAST((SUM(TotalRevenuePerTaxJurisdiction) * 100/ TotalRevenuePerYear) AS DECIMAL(20,2)) AS YearlyRevenuePercentageOfGroupRevenue,
		COUNT(TaxJurisdiction) AS TotalTaxJurisdictions
		FROM CbcReportDataCountCTE
		GROUP BY ReportableAccountingYear, TotalRevenuePerYear, IncomeTaxAccruedRAYPerYear
		ORDER BY ReportableAccountingYear DESC;

END
GO
/****** Object:  StoredProcedure [dbo].[GetRelatedPartyRevenueSnapshotOfnthYearsDrillDown]    Script Date: 1/4/2024 3:21:24 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[GetRelatedPartyRevenueSnapshotOfnthYearsDrillDown]
@DrillCountry NVARCHAR(100),
@SelectType INT,
@Periods NVARCHAR(MAX) = NULL,
@Countries NVARCHAR(MAX) = NULL,
@Entities NVARCHAR(MAX) = NULL,
@CurrencyType NVARCHAR(10)
AS
BEGIN

		SET NOCOUNT ON;
		DECLARE @DYNAMICPIVOTQUERYREVENUE AS NVARCHAR(MAX);
		DECLARE @DYNAMICPIVOTQUERYPERCENTAGE AS NVARCHAR(MAX);
		DECLARE @PIVOTCOLUMNNAMESREVENUE AS NVARCHAR(MAX);
		DECLARE @PIVOTCOLUMNNAMESPERCENTAGE AS NVARCHAR(MAX);
		DECLARE @FYPERIODLIST AS NVARCHAR(MAX);
		DECLARE @CURRENTYEAR AS NVARCHAR(50) ='FY_' +  REPLACE(@Periods, '-', '_');

		DROP TABLE IF EXISTS #TempPeriods;
		DROP TABLE IF EXISTS #TempCountries;
		DROP TABLE IF EXISTS #TempEntities;
		DROP TABLE IF EXISTS #CbcFileUpload;
		DROP TABLE IF EXISTS #CbcFileUpload1;

		DROP TABLE IF EXISTS #CbcReportDataCurrencyWise;
		SELECT ClientId,NameOfGroup,ReportableAccountingYear,Period,Currency,ConstituentEntity,CorporateTaxRate,TaxJurisdiction,
		CAST((UnrelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS UnrelatedPartyRevenue,
		CAST((RelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS RelatedPartyRevenue,
		CAST((TotalRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS TotalRevenue,
		CAST((ProfitLossBeforeIncomeTax * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS ProfitLossBeforeIncomeTax,
		CAST((IncomeTaxPaidOnCashBasis * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxPaidOnCashBasis,
		CAST((IncomeTaxAccruedRAY * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxAccruedRAY,
		CAST((StatedCapital * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS StatedCapital,
		CAST((AccumulatedEarnings * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS AccumulatedEarnings,
		NumberOfEmployees,TangibleAssets,FileName,FileFullName,CRD.CreatedBy,CRD.CreatedDate
		INTO #CbcReportDataCurrencyWise
		FROM CbcReportData AS CRD
		JOIN CbCRCurrencyConversionData AS CCD ON CRD.Currency=CCD.CurrencyFROM
		WHERE CCD.CurrencyTo=@CurrencyType;

		CREATE TABLE #TempPeriods (Item NVARCHAR(255));
		CREATE TABLE #TempCountries (Item NVARCHAR(255));
		CREATE TABLE #TempEntities (Item NVARCHAR(255));

		IF(@Periods IS NULL)
		BEGIN
			SELECT DISTINCT TOP 1 @Periods= ReportableAccountingYear FROM #CbcReportDataCurrencyWise  ORDER BY ReportableAccountingYear DESC;
		END
		
		IF(@Periods IS NOT NULL)
			BEGIN
				INSERT INTO #TempPeriods (Item)
				SELECT DISTINCT ReportableAccountingYear AS Item
				FROM #CbcReportDataCurrencyWise 
				WHERE CAST(SUBSTRING(ReportableAccountingYear, 1, 4) AS INT) BETWEEN CAST(SUBSTRING(@Periods, 1, 4) AS INT) - 5 AND CAST(SUBSTRING(@Periods, 1, 4) AS INT)
				ORDER BY ReportableAccountingYear DESC;
			END;
		
		IF(@Countries IS NOT NULL)
			BEGIN
				INSERT INTO #TempCountries (Item)
				SELECT Item
				FROM dbo.SplitStringToTable(@Countries, ',');
			END
		ELSE
			BEGIN
				INSERT INTO #TempCountries (Item)
				SELECT DISTINCT TaxJurisdiction AS Item
				FROM #CbcReportDataCurrencyWise 
				WHERE ReportableAccountingYear IN (SELECT Item FROM #TempPeriods);
			END;

		IF(@Entities IS NOT NULL)
			BEGIN
				INSERT INTO #TempEntities (Item)
				SELECT Item
				FROM dbo.SplitStringToTable(@Entities, ',');
			END
		ELSE
			BEGIN
				INSERT INTO #TempEntities (Item)
				SELECT DISTINCT ConstituentEntity AS Item
				FROM #CbcReportDataCurrencyWise
				WHERE TaxJurisdiction IN (SELECT Item FROM #TempCountries);
			END;

		--BY VALUE TYPE
		IF(@SelectType = 1)
		BEGIN
			-- Created  Temp Table...
			SELECT DISTINCT ReportableAccountingYear, TaxJurisdiction, ConstituentEntity,
			SUM(RelatedPartyRevenue) AS RPROfJurisdiction
			INTO #CbcFileUpload
			FROM #CbcReportDataCurrencyWise 
			WHERE TotalRevenue <> 0 
				AND LOWER(TaxJurisdiction) = LOWER(@DrillCountry)
				AND TaxJurisdiction IN (SELECT Item FROM #TempCountries) 
				AND (ConstituentEntity IN (SELECT Item FROM #TempEntities) OR ConstituentEntity IS NULL)
			GROUP BY ReportableAccountingYear, TaxJurisdiction, ConstituentEntity
			ORDER BY ReportableAccountingYear DESC;

			-- Get Distinct FY Period For dynamic Pivot...
			SELECT @FYPERIODLIST= COALESCE(@FYPERIODLIST+',','')+ QUOTENAME(ReportableAccountingYear) 
			FROM (SELECT DISTINCT ReportableAccountingYear FROM #CbcFileUpload) AS SUBQUERY;

			-- Set Pivot Column Names Revenue
			SET @PIVOTCOLUMNNAMESREVENUE = STUFF((
						SELECT DISTINCT ',' + 
						'ISNULL(' + QUOTENAME(ReportableAccountingYear)+ ', 0) AS FY_' +  REPLACE(ReportableAccountingYear, '-', '_')
						FROM (SELECT DISTINCT ReportableAccountingYear FROM #CbcFileUpload) AS SUBQUERY
						FOR XML PATH('')
					), 1, 1, '');

			-- Set Dynamic Pivot Query Revenue
			SET @DYNAMICPIVOTQUERYREVENUE = N'
			SELECT * FROM (
					SELECT REPLACE(ConstituentEntity, '' '', '+'''\n'''+') AS ConstituentEntity, TaxJurisdiction, ' + @PIVOTCOLUMNNAMESREVENUE + '
				FROM #CbcFileUpload
				PIVOT (MAX(RPROfJurisdiction) FOR ReportableAccountingYear IN (' + @FYPERIODLIST + ')) AS PivotTable
			) AS PV ORDER BY ' + @CURRENTYEAR+ ' DESC ;';

			PRINT @DYNAMICPIVOTQUERYREVENUE;
			EXEC sp_executesql @DYNAMICPIVOTQUERYREVENUE;
		END
		ELSE
		BEGIN
			-- Created  Temp Table...
			SELECT DISTINCT ReportableAccountingYear, TaxJurisdiction, ConstituentEntity,
			CAST(((CAST(SUM(RelatedPartyRevenue) AS DECIMAL(19,2)) * 100 / CAST(SUM(TotalRevenue) AS DECIMAL(19,2)))) AS DECIMAL(19,2)) AS RPRPercentageOfJurisdictionRevenue
			INTO #CbcFileUpload1
			FROM #CbcReportDataCurrencyWise 
			WHERE TotalRevenue <> 0 
				AND LOWER(TaxJurisdiction) = LOWER(@DrillCountry)
				AND TaxJurisdiction IN (SELECT Item FROM #TempCountries) 
				AND (ConstituentEntity IN (SELECT Item FROM #TempEntities) OR ConstituentEntity IS NULL)
			GROUP BY ReportableAccountingYear, TaxJurisdiction, ConstituentEntity
			ORDER BY ReportableAccountingYear DESC;

			-- Get Distinct FY Period For dynamic Pivot...
			SELECT @FYPERIODLIST= COALESCE(@FYPERIODLIST+',','')+ QUOTENAME(ReportableAccountingYear) 
			FROM (SELECT DISTINCT ReportableAccountingYear FROM #CbcFileUpload1) AS SUBQUERY;

			SET @PIVOTCOLUMNNAMESPERCENTAGE = STUFF((
						SELECT DISTINCT ',' + 
						'ISNULL(' + QUOTENAME(ReportableAccountingYear)+ ', 0) AS FY_' +  REPLACE(ReportableAccountingYear, '-', '_')
						FROM (SELECT DISTINCT ReportableAccountingYear FROM #CbcFileUpload1) AS SUBQUERY
						FOR XML PATH('')
					), 1, 1, '');
		
			-- Set Dynamic Pivot Query Revenue
			SET @DYNAMICPIVOTQUERYPERCENTAGE = N'
				SELECT * FROM (
					SELECT REPLACE(ConstituentEntity, '' '', '+'''\n'''+') AS ConstituentEntity, TaxJurisdiction, ' + @PIVOTCOLUMNNAMESPERCENTAGE + '
					FROM #CbcFileUpload1
					PIVOT (MAX(RPRPercentageOfJurisdictionRevenue) FOR ReportableAccountingYear IN (' + @FYPERIODLIST + ')) AS PivotTable
				) AS PV ORDER BY ' + @CURRENTYEAR+ ' DESC ;';

			PRINT @DYNAMICPIVOTQUERYPERCENTAGE;
			EXEC sp_executesql @DYNAMICPIVOTQUERYPERCENTAGE;
		END;

		WITH CbcReportDataCountCTE AS (
			SELECT DISTINCT
			ReportableAccountingYear,
			TaxJurisdiction,
			SUM(TotalRevenue) AS TotalRevenuePerTaxJurisdiction,
			SUM(IncomeTaxAccruedRAY) AS IncomeTaxAccruedRAYPerTaxJurisdiction,
			SUM(SUM(IncomeTaxAccruedRAY)) OVER(PARTITION BY ReportableAccountingYear) AS IncomeTaxAccruedRAYPerYear,
			SUM(SUM(TotalRevenue)) OVER (PARTITION BY ReportableAccountingYear) AS TotalRevenuePerYear
			FROM #CbcReportDataCurrencyWise
			WHERE TotalRevenue <> 0 
					AND ReportableAccountingYear = @Periods
					AND TaxJurisdiction IN (SELECT Item FROM #TempCountries) 
					AND (ConstituentEntity IN (SELECT Item FROM #TempEntities) OR ConstituentEntity IS NULL)
			GROUP BY ReportableAccountingYear, TaxJurisdiction
		)
		SELECT ReportableAccountingYear, 
		SUM(TotalRevenuePerTaxJurisdiction) AS TotalRevenueOfAllJurisdictionPerYear,
		SUM(IncomeTaxAccruedRAYPerTaxJurisdiction) AS IncomeTaxAccruedRAYPerYear,
		CAST((SUM(TotalRevenuePerTaxJurisdiction) * 100/ TotalRevenuePerYear) AS DECIMAL(20,2)) AS YearlyRevenuePercentageOfGroupRevenue,
		COUNT(TaxJurisdiction) AS TotalTaxJurisdictions
		FROM CbcReportDataCountCTE
		WHERE LOWER(TaxJurisdiction) = LOWER(@DrillCountry)
		GROUP BY ReportableAccountingYear, TotalRevenuePerYear, IncomeTaxAccruedRAYPerYear
		ORDER BY ReportableAccountingYear DESC;
END
GO
/****** Object:  StoredProcedure [dbo].[GetRevenueSnapshotOfnthYears]    Script Date: 1/4/2024 3:21:24 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[GetRevenueSnapshotOfnthYears]
@SelectType INT,
@Periods NVARCHAR(MAX) = NULL,
@Countries NVARCHAR(MAX) = NULL,
@Entities NVARCHAR(MAX) = NULL,
@CurrencyType NVARCHAR(10)
AS
BEGIN
		SET NOCOUNT ON;

		DECLARE @DYNAMICPIVOTQUERY AS NVARCHAR(MAX);
		DECLARE @PIVOTCOLUMNNAMES AS NVARCHAR(MAX);
		DECLARE @FYPERIODLIST AS NVARCHAR(MAX);
		DECLARE @CURRENTYEAR AS NVARCHAR(50) ='FY_' +  REPLACE(@Periods, '-', '_');

		DROP TABLE IF EXISTS #TempPeriods;
		DROP TABLE IF EXISTS #TempCountries;
		DROP TABLE IF EXISTS #TempEntities;
		DROP TABLE IF EXISTS #CbcFileUpload;
		DROP TABLE IF EXISTS #CbcFileUpload1;

		DROP TABLE IF EXISTS #CbcReportDataCurrencyWise;
		SELECT ClientId,NameOfGroup,ReportableAccountingYear,Period,Currency,ConstituentEntity,CorporateTaxRate,TaxJurisdiction,
		CAST((UnrelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS UnrelatedPartyRevenue,
		CAST((RelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS RelatedPartyRevenue,
		CAST((TotalRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS TotalRevenue,
		CAST((ProfitLossBeforeIncomeTax * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS ProfitLossBeforeIncomeTax,
		CAST((IncomeTaxPaidOnCashBasis * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxPaidOnCashBasis,
		CAST((IncomeTaxAccruedRAY * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxAccruedRAY,
		CAST((StatedCapital * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS StatedCapital,
		CAST((AccumulatedEarnings * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS AccumulatedEarnings,
		NumberOfEmployees,TangibleAssets,FileName,FileFullName,CRD.CreatedBy,CRD.CreatedDate
		INTO #CbcReportDataCurrencyWise
		FROM CbcReportData AS CRD
		JOIN CbCRCurrencyConversionData AS CCD ON CRD.Currency=CCD.CurrencyFROM
		WHERE CCD.CurrencyTo=@CurrencyType;

		CREATE TABLE #TempPeriods (ID INT IDENTITY(1,1), Item NVARCHAR(255), PRIMARY KEY(ID));
		CREATE TABLE #TempCountries (Item NVARCHAR(255));
		CREATE TABLE #TempEntities (Item NVARCHAR(255));

		IF(@Periods IS NULL)
		BEGIN
			SELECT DISTINCT TOP 1 @Periods= ReportableAccountingYear FROM #CbcReportDataCurrencyWise  ORDER BY ReportableAccountingYear DESC;
		END
		
		IF(@Periods IS NOT NULL)
			BEGIN
				INSERT INTO #TempPeriods (Item)
				SELECT DISTINCT ReportableAccountingYear AS Item
				FROM #CbcReportDataCurrencyWise 
				WHERE CAST(SUBSTRING(ReportableAccountingYear, 1, 4) AS INT) BETWEEN CAST(SUBSTRING(@Periods, 1, 4) AS INT) - 5 AND CAST(SUBSTRING(@Periods, 1, 4) AS INT)
				ORDER BY ReportableAccountingYear DESC;
			END;
		
		IF(@Countries IS NOT NULL)
			BEGIN
				INSERT INTO #TempCountries (Item)
				SELECT Item
				FROM dbo.SplitStringToTable(@Countries, ',');
			END
		ELSE
			BEGIN
				INSERT INTO #TempCountries (Item)
				SELECT DISTINCT TaxJurisdiction AS Item
				FROM #CbcReportDataCurrencyWise 
				WHERE ReportableAccountingYear IN (SELECT Item FROM #TempPeriods);
			END;

		IF(@Entities IS NOT NULL)
			BEGIN
				INSERT INTO #TempEntities (Item)
				SELECT Item
				FROM dbo.SplitStringToTable(@Entities, ',');
			END
		ELSE
			BEGIN
				INSERT INTO #TempEntities (Item)
				SELECT DISTINCT ConstituentEntity AS Item
				FROM #CbcReportDataCurrencyWise
				WHERE TaxJurisdiction IN (SELECT Item FROM #TempCountries);
			END;

		IF(@SelectType = 1)
			BEGIN
				WITH CbcReportDataCountCTE AS (
					SELECT DISTINCT
						ReportableAccountingYear,
						TaxJurisdiction,
						SUM(TotalRevenue) AS TotalRevenuePerTaxJurisdiction,
						SUM(SUM(IncomeTaxAccruedRAY)) OVER(PARTITION BY ReportableAccountingYear) AS IncomeTaxAccruedRAYPerYear,
						SUM(SUM(TotalRevenue)) OVER (PARTITION BY ReportableAccountingYear) AS TotalRevenuePerYear
					FROM #CbcReportDataCurrencyWise
					WHERE TotalRevenue <> 0 
						AND ReportableAccountingYear IN (SELECT Item FROM #TempPeriods)
						AND TaxJurisdiction IN (SELECT Item FROM #TempCountries) 
						AND (ConstituentEntity IN (SELECT Item FROM #TempEntities) OR ConstituentEntity IS NULL)
					GROUP BY ReportableAccountingYear, TaxJurisdiction
				)
				SELECT ReportableAccountingYear, TaxJurisdiction, 
				SUM(TotalRevenuePerTaxJurisdiction) AS JurisdictionRevenue
				INTO #CbcFileUpload
				FROM CbcReportDataCountCTE
				GROUP BY ReportableAccountingYear, TaxJurisdiction 

				-- Get Distinct FY Period For dynamic Pivot...
				SELECT @FYPERIODLIST= COALESCE(@FYPERIODLIST+',','')+ QUOTENAME(Item) 
				FROM (SELECT DISTINCT Item FROM #TempPeriods) AS SUBQUERY;

				-- Set Pivot Column Names
				SET @PIVOTCOLUMNNAMES = STUFF((
							SELECT DISTINCT ',' + 
							'ISNULL(' + QUOTENAME(Item)+ ', 0) AS FY_' +  REPLACE(Item, '-', '_')
							FROM (SELECT DISTINCT Item FROM #TempPeriods) AS SUBQUERY
							FOR XML PATH('')
						), 1, 1, '');

				-- Set Dynamic Pivot Query
				SET @DYNAMICPIVOTQUERY = N'
				SELECT * FROM (
					SELECT TaxJurisdiction, ' + @PIVOTCOLUMNNAMES + '
					FROM #CbcFileUpload 
					PIVOT (MAX(JurisdictionRevenue) FOR ReportableAccountingYear IN (' + @FYPERIODLIST + ')) AS PivotTable
				) AS PV ORDER BY ' + @CURRENTYEAR+ ' DESC ;';

				PRINT @DYNAMICPIVOTQUERY;
				EXEC sp_executesql @DYNAMICPIVOTQUERY;
			END
		ELSE
			BEGIN
				WITH CbcReportDataCountCTE AS (
					SELECT DISTINCT
					ReportableAccountingYear,
					TaxJurisdiction,
					SUM(TotalRevenue) AS TotalRevenuePerTaxJurisdiction,
					SUM(SUM(IncomeTaxAccruedRAY)) OVER(PARTITION BY ReportableAccountingYear) AS IncomeTaxAccruedRAYPerYear,
					SUM(SUM(TotalRevenue)) OVER (PARTITION BY ReportableAccountingYear) AS TotalRevenuePerYear
					FROM #CbcReportDataCurrencyWise
					WHERE TotalRevenue <> 0 
						AND ReportableAccountingYear IN (SELECT Item FROM #TempPeriods)
						AND TaxJurisdiction IN (SELECT Item FROM #TempCountries) 
						AND (ConstituentEntity IN (SELECT Item FROM #TempEntities) OR ConstituentEntity IS NULL)
					GROUP BY ReportableAccountingYear, TaxJurisdiction
				)
				SELECT ReportableAccountingYear, TaxJurisdiction, 
				CAST(((CAST(SUM(TotalRevenuePerTaxJurisdiction) AS DECIMAL(19,2)) * 100 / CAST(SUM(TotalRevenuePerYear) AS DECIMAL(19,2)))) AS DECIMAL(19,2)) AS RevenuePercentageOfGroupRevenue
				INTO #CbcFileUpload1
				FROM CbcReportDataCountCTE
				GROUP BY ReportableAccountingYear, TaxJurisdiction, TotalRevenuePerYear

				-- Get Distinct FY Period For dynamic Pivot...
				SELECT @FYPERIODLIST= COALESCE(@FYPERIODLIST+',','')+ QUOTENAME(item) 
				FROM (SELECT DISTINCT item FROM #TempPeriods) AS SUBQUERY;

				-- Set Pivot Column Names
				SET @PIVOTCOLUMNNAMES = STUFF((
							SELECT DISTINCT ',' + 
							'ISNULL(' + QUOTENAME(Item)+ ', 0) AS FY_' +  REPLACE(Item, '-', '_')
							FROM (SELECT DISTINCT Item FROM #TempPeriods) AS SUBQUERY
							FOR XML PATH('')
						), 1, 1, '');
		
				-- Set Dynamic Pivot Query
				SET @DYNAMICPIVOTQUERY = N'
				SELECT * FROM (
					SELECT TaxJurisdiction, ' + @PIVOTCOLUMNNAMES + '
					FROM #CbcFileUpload1
					PIVOT (MAX(RevenuePercentageOfGroupRevenue) FOR ReportableAccountingYear IN (' + @FYPERIODLIST + ')) AS PivotTable
				) AS PV ORDER BY ' + @CURRENTYEAR+ ' DESC ;';

				PRINT @DYNAMICPIVOTQUERY;
				EXEC sp_executesql @DYNAMICPIVOTQUERY;
			END;

		WITH CbcReportDataCountCTE AS (
			SELECT DISTINCT
			ReportableAccountingYear,
			TaxJurisdiction,
			SUM(TotalRevenue) AS TotalRevenuePerTaxJurisdiction,
			SUM(IncomeTaxAccruedRAY) AS IncomeTaxAccruedRAYPerTaxJurisdiction,
			SUM(SUM(IncomeTaxAccruedRAY)) OVER(PARTITION BY ReportableAccountingYear) AS IncomeTaxAccruedRAYPerYear,
			SUM(SUM(TotalRevenue)) OVER (PARTITION BY ReportableAccountingYear) AS TotalRevenuePerYear
			FROM #CbcReportDataCurrencyWise
			WHERE TotalRevenue <> 0 
					AND ReportableAccountingYear = @Periods
					AND TaxJurisdiction IN (SELECT Item FROM #TempCountries) 
					AND (ConstituentEntity IN (SELECT Item FROM #TempEntities) OR ConstituentEntity IS NULL)
			GROUP BY ReportableAccountingYear, TaxJurisdiction
		)
		SELECT ReportableAccountingYear,
		SUM(TotalRevenuePerTaxJurisdiction) AS TotalRevenueOfAllJurisdictionPerYear,
		SUM(IncomeTaxAccruedRAYPerTaxJurisdiction) AS IncomeTaxAccruedRAYPerYear,
		CAST((SUM(TotalRevenuePerTaxJurisdiction) * 100/ TotalRevenuePerYear) AS DECIMAL(20,2)) AS YearlyRevenuePercentageOfGroupRevenue,
		COUNT(TaxJurisdiction) AS TotalTaxJurisdictions
		FROM CbcReportDataCountCTE
		GROUP BY ReportableAccountingYear, TotalRevenuePerYear, IncomeTaxAccruedRAYPerYear
		ORDER BY ReportableAccountingYear DESC;

END
GO
/****** Object:  StoredProcedure [dbo].[GetRevenueSnapshotOfnthYearsDrillDown]    Script Date: 1/4/2024 3:21:24 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

CREATE PROCEDURE [dbo].[GetRevenueSnapshotOfnthYearsDrillDown]
@DrillCountry NVARCHAR(100),
@SelectType INT,
@Periods NVARCHAR(MAX) = NULL,
@Countries NVARCHAR(MAX) = NULL,
@Entities NVARCHAR(MAX) = NULL,
@CurrencyType NVARCHAR(10)
AS
BEGIN
		SET NOCOUNT ON;

		DECLARE @DYNAMICPIVOTQUERY AS NVARCHAR(MAX);
		DECLARE @PIVOTCOLUMNNAMES AS NVARCHAR(MAX);
		DECLARE @FYPERIODLIST AS NVARCHAR(MAX);
		DECLARE @CURRENTYEAR AS NVARCHAR(50) ='FY_' +  REPLACE(@Periods, '-', '_');

		DROP TABLE IF EXISTS #TempPeriods;
		DROP TABLE IF EXISTS #TempCountries;
		DROP TABLE IF EXISTS #TempEntities;
		DROP TABLE IF EXISTS #CbcFileUpload;
		DROP TABLE IF EXISTS #CbcFileUpload1;

		DROP TABLE IF EXISTS #CbcReportDataCurrencyWise;
		SELECT ClientId,NameOfGroup,ReportableAccountingYear,Period,Currency,ConstituentEntity,CorporateTaxRate,TaxJurisdiction,
		CAST((UnrelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS UnrelatedPartyRevenue,
		CAST((RelatedPartyRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS RelatedPartyRevenue,
		CAST((TotalRevenue * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS TotalRevenue,
		CAST((ProfitLossBeforeIncomeTax * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS ProfitLossBeforeIncomeTax,
		CAST((IncomeTaxPaidOnCashBasis * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxPaidOnCashBasis,
		CAST((IncomeTaxAccruedRAY * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS IncomeTaxAccruedRAY,
		CAST((StatedCapital * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS StatedCapital,
		CAST((AccumulatedEarnings * CCD.CurrencyToValue) AS DECIMAL(20,0)) AS AccumulatedEarnings,
		NumberOfEmployees,TangibleAssets,FileName,FileFullName,CRD.CreatedBy,CRD.CreatedDate
		INTO #CbcReportDataCurrencyWise
		FROM CbcReportData AS CRD
		JOIN CbCRCurrencyConversionData AS CCD ON CRD.Currency=CCD.CurrencyFROM
		WHERE CCD.CurrencyTo=@CurrencyType;

		CREATE TABLE #TempPeriods (Item NVARCHAR(255));
		CREATE TABLE #TempCountries (Item NVARCHAR(255));
		CREATE TABLE #TempEntities (Item NVARCHAR(255));

		IF(@Periods IS NULL)
		BEGIN
			SELECT DISTINCT TOP 1 @Periods= ReportableAccountingYear FROM #CbcReportDataCurrencyWise  ORDER BY ReportableAccountingYear DESC;
		END
		
		IF(@Periods IS NOT NULL)
			BEGIN
				INSERT INTO #TempPeriods (Item)
				SELECT DISTINCT ReportableAccountingYear AS Item
				FROM #CbcReportDataCurrencyWise 
				WHERE CAST(SUBSTRING(ReportableAccountingYear, 1, 4) AS INT) BETWEEN CAST(SUBSTRING(@Periods, 1, 4) AS INT) - 5 AND CAST(SUBSTRING(@Periods, 1, 4) AS INT)
				ORDER BY ReportableAccountingYear DESC;
			END;
		
		IF(@Countries IS NOT NULL)
			BEGIN
				INSERT INTO #TempCountries (Item)
				SELECT Item
				FROM dbo.SplitStringToTable(@Countries, ',');
			END
		ELSE
			BEGIN
				INSERT INTO #TempCountries (Item)
				SELECT DISTINCT TaxJurisdiction AS Item
				FROM #CbcReportDataCurrencyWise 
				WHERE ReportableAccountingYear IN (SELECT Item FROM #TempPeriods);
			END;

		IF(@Entities IS NOT NULL)
			BEGIN
				INSERT INTO #TempEntities (Item)
				SELECT Item
				FROM dbo.SplitStringToTable(@Entities, ',');
			END
		ELSE
			BEGIN
				INSERT INTO #TempEntities (Item)
				SELECT DISTINCT ConstituentEntity AS Item
				FROM #CbcReportDataCurrencyWise
				WHERE TaxJurisdiction IN (SELECT Item FROM #TempCountries);
			END;

		IF(@SelectType = 1)
			BEGIN
				-- Created  Temp Table...
				SELECT ConstituentEntity, ReportableAccountingYear, TaxJurisdiction, SUM( TotalRevenue ) AS JurisdictionRevenue
				INTO #CbcFileUpload
				FROM #CbcReportDataCurrencyWise 
				WHERE TotalRevenue <> 0 
					AND LOWER(TaxJurisdiction) = LOWER(@DrillCountry)
					AND TaxJurisdiction IN (SELECT Item FROM #TempCountries) 
					AND (ConstituentEntity IN (SELECT Item FROM #TempEntities) OR ConstituentEntity IS NULL)
				GROUP BY ConstituentEntity, ReportableAccountingYear, TaxJurisdiction 
				ORDER BY ConstituentEntity, ReportableAccountingYear, TaxJurisdiction DESC;

				-- Get Distinct FY Period For dynamic Pivot...
				SELECT @FYPERIODLIST= COALESCE(@FYPERIODLIST+',','')+ QUOTENAME(ReportableAccountingYear) 
				FROM (SELECT DISTINCT ReportableAccountingYear FROM #CbcFileUpload) AS SUBQUERY;

				-- Set Pivot Column Names
				SET @PIVOTCOLUMNNAMES = STUFF((
							SELECT DISTINCT ',' + 
							'ISNULL(' + QUOTENAME(ReportableAccountingYear)+ ', 0) AS FY_' +  REPLACE(ReportableAccountingYear, '-', '_')
							FROM (SELECT DISTINCT ReportableAccountingYear FROM #CbcFileUpload) AS SUBQUERY
							FOR XML PATH('')
						), 1, 1, '');
		
				-- Set Dynamic Pivot Query
				SET @DYNAMICPIVOTQUERY = N'
				SELECT * FROM (
					SELECT REPLACE(ConstituentEntity, '' '', '+'''\n'''+') AS ConstituentEntity, TaxJurisdiction, ' + @PIVOTCOLUMNNAMES + '
					FROM #CbcFileUpload 
					PIVOT (MAX(JurisdictionRevenue) FOR ReportableAccountingYear IN (' + @FYPERIODLIST + ')) AS PivotTable
				) AS PV ORDER BY ' + @CURRENTYEAR+ ' DESC ;';

				PRINT @DYNAMICPIVOTQUERY;
				EXEC sp_executesql @DYNAMICPIVOTQUERY;
			END
		ELSE
			BEGIN
				-- Created  Temp Table...
				SELECT ConstituentEntity, ReportableAccountingYear, TaxJurisdiction, 
				CAST(((CAST(SUM(TotalRevenue) AS DECIMAL(19,2)) * 100 / CAST(SUM(SUM(TotalRevenue)) OVER(PARTITION BY TaxJurisdiction) AS DECIMAL(19,2)))) AS DECIMAL(19,2)) AS RevenuePercentageOfGroupRevenue
				INTO #CbcFileUpload1
				FROM #CbcReportDataCurrencyWise 
				WHERE TotalRevenue <> 0 
					AND LOWER(TaxJurisdiction) = LOWER(@DrillCountry)
					AND TaxJurisdiction IN (SELECT Item FROM #TempCountries) 
					AND (ConstituentEntity IN (SELECT Item FROM #TempEntities) OR ConstituentEntity IS NULL)
				GROUP BY ConstituentEntity, ReportableAccountingYear, TaxJurisdiction
				ORDER BY ConstituentEntity, ReportableAccountingYear, TaxJurisdiction DESC;

				-- Get Distinct FY Period For dynamic Pivot...
				SELECT @FYPERIODLIST= COALESCE(@FYPERIODLIST+',','')+ QUOTENAME(ReportableAccountingYear) 
				FROM (SELECT DISTINCT ReportableAccountingYear FROM #CbcFileUpload1) AS SUBQUERY;

				-- Set Pivot Column Names
				SET @PIVOTCOLUMNNAMES = STUFF((
							SELECT DISTINCT ',' + 
							'ISNULL(' + QUOTENAME(ReportableAccountingYear)+ ', 0) AS FY_' +  REPLACE(ReportableAccountingYear, '-', '_')
							FROM (SELECT DISTINCT ReportableAccountingYear FROM #CbcFileUpload1) AS SUBQUERY
							FOR XML PATH('')
						), 1, 1, '');
		
				-- Set Dynamic Pivot Query
				SET @DYNAMICPIVOTQUERY = N'
				SELECT * FROM (
					SELECT REPLACE(ConstituentEntity, '' '', '+'''\n'''+') AS ConstituentEntity, TaxJurisdiction, ' + @PIVOTCOLUMNNAMES + '
					FROM #CbcFileUpload1
					PIVOT (MAX(RevenuePercentageOfGroupRevenue) FOR ReportableAccountingYear IN (' + @FYPERIODLIST + ')) AS PivotTable
				) AS PV ORDER BY ' + @CURRENTYEAR+ ' DESC ;';

				PRINT @DYNAMICPIVOTQUERY;
				EXEC sp_executesql @DYNAMICPIVOTQUERY;
			END;

		WITH CbcReportDataCountCTE AS (
			SELECT DISTINCT
			ReportableAccountingYear,
			TaxJurisdiction,
			SUM(TotalRevenue) AS TotalRevenuePerTaxJurisdiction,
			SUM(IncomeTaxAccruedRAY) AS IncomeTaxAccruedRAYPerTaxJurisdiction,
			SUM(SUM(IncomeTaxAccruedRAY)) OVER(PARTITION BY ReportableAccountingYear) AS IncomeTaxAccruedRAYPerYear,
			SUM(SUM(TotalRevenue)) OVER (PARTITION BY ReportableAccountingYear) AS TotalRevenuePerYear
			FROM #CbcReportDataCurrencyWise
			WHERE TotalRevenue <> 0 
					AND ReportableAccountingYear = @Periods
					AND TaxJurisdiction IN (SELECT Item FROM #TempCountries) 
					AND (ConstituentEntity IN (SELECT Item FROM #TempEntities) OR ConstituentEntity IS NULL)
			GROUP BY ReportableAccountingYear, TaxJurisdiction
		)
		SELECT ReportableAccountingYear,
		SUM(TotalRevenuePerTaxJurisdiction) AS TotalRevenueOfAllJurisdictionPerYear,
		SUM(IncomeTaxAccruedRAYPerTaxJurisdiction) AS IncomeTaxAccruedRAYPerYear,
		CAST((SUM(TotalRevenuePerTaxJurisdiction) * 100/ TotalRevenuePerYear) AS DECIMAL(20,2)) AS YearlyRevenuePercentageOfGroupRevenue,
		COUNT(TaxJurisdiction) AS TotalTaxJurisdictions
		FROM CbcReportDataCountCTE
		WHERE LOWER(TaxJurisdiction) = LOWER(@DrillCountry)
		GROUP BY ReportableAccountingYear, TotalRevenuePerYear, IncomeTaxAccruedRAYPerYear
		ORDER BY ReportableAccountingYear DESC;
END
GO
/****** Object:  StoredProcedure [dbo].[SaveUpdateCurrencyDetails]    Script Date: 1/4/2024 3:21:24 PM ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO
CREATE PROCEDURE [dbo].[SaveUpdateCurrencyDetails] 
@CurrencyFrom NVARCHAR(5),
@CurrencyTo NVARCHAR(5), 
@CurrencyFromValue NVARCHAR(50), 
@CurrencyToValue NVARCHAR(50), 
@CreatedBy NVARCHAR(512), 
@IsDeleteTable BIT 
AS 
BEGIN
    SET NOCOUNT ON;
	IF(@IsDeleteTable = 1) 
		BEGIN
			TRUNCATE TABLE [CbCRCurrencyConversionData];
			DBCC CHECKIDENT ('CbCRCurrencyConversionData', RESEED, 1);
		END;

	PRINT @CurrencyToValue;
	INSERT INTO [dbo].[CbCRCurrencyConversionData] ([CurrencyFrom] , [CurrencyTo] , [CurrencyFromValue] , [CurrencyToValue] , [CreatedBy] , [ModifiedBy] , [CreationDate] , [ModifiedDate]) 
	VALUES (@CurrencyFrom, @CurrencyTo, CAST(@CurrencyFromValue AS DECIMAL(19,9)), CAST(@CurrencyToValue  AS DECIMAL(19,9)), @CreatedBy, @CreatedBy, GETDATE(), GETDATE())
END;
GO
